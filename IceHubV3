Prevail.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Prevail.Position = UDim2.new(0.565737069, 0, 0.586028755, 0)
Prevail.Size = UDim2.new(0, 71, 0, 50)
Prevail.Font = Enum.Font.SciFi
Prevail.Text = "Prevail"
